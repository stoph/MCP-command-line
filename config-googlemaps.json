{
  "server": "npx -y @modelcontextprotocol/server-google-maps",
  "env": {
    "GOOGLE_MAPS_API_KEY": "your-api-key",
    "DEBUG": "true"
  },
  "tools": [
    {
      "name": "maps_search_places",
      "arguments": {
        "query": "Golden Gate Bridge"
      }
    },
    {
      "name": "maps_geocode",
      "arguments": {
        "address": "1600 Pennsylvania Avenue NW, Washington, DC"
      }
    }
  ],
  "options": {
    "verbose": false,
    "list_only": false,
    "interactive": false
  }
} 